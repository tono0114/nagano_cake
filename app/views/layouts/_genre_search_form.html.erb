<table width="180px" height="300px" class="genre_search_form">
  <thead class="table-active search_form_head text-center">
    <th>ジャンル検索</th>
  </thead>
  <tbody class="genre_type">
    <% @genres.each do |genre| %>
    <tr>
      <td><p class="m-2"><%= genre.name %></p></td>
    </tr>
    <% end %>
  </tbody>
</table>